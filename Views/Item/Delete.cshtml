@model Item

<h2>Delete Item</h2>

<h3>Are you sure you want to delete this item?</h3>
<div>
    <h4>Item</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Item Id</dt>
        <dd class="col-sm-10">@Model.ItemId</dd>

        <dt class="col-sm-2">Name</dt>
        <dd class="col-sm-10">@Model.Name</dd>

        <dt class="col-sm-2">Food group</dt>
        <dd class="col-sm-10">@Model.Food_Group</dd>

        <dt class="col-sm-2">Energi per 100g</dt>
        <dd class="col-sm-10">@Model.Energi_Kcal</dd>

        <dt class="col-sm-2">Fett per 100g</dt>
        <dd class="col-sm-10">@Model.Fett</dd>

        <dt class="col-sm-2">Protein per 100g</dt>
        <dd class="col-sm-10">@Model.Protein</dd>

        <dt class="col-sm-2">Karbohydrat per 100g</dt>
        <dd class="col-sm-10">@Model.Karbohydrat</dd>

        <dt class="col-sm-2">Salt per 100g</dt>
        <dd class="col-sm-10">@Model.Salt</dd>
       

    
        <dt class="col-sm-2">Image URL</dt>
        <dd class="col-sm-10">@Model.ImageUrl</dd>
    </dl>

    <form asp-action="DeleteConfirmed" asp-route-id="@Model.ItemId" method="post">
        <button type="submit" class="btn btn-danger">Delete</button>
        <a asp-action="Table" class="btn btn-secondary">Cancel</a>
    </form>
</div>